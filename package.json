{
  "name": "typescript-graphql-request-with-retries",
  "version": "0.1.1",
  "description": "GraphQL Code Generator plugin for generating a ready-to-use SDK based on graphql-request and GraphQL operations",
  "license": "MIT",
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "prepack": "bob-update-version",
    "build": "bob",
    "test": "jest --no-watchman"
  },
  "peerDependenxcies": {
    "graphql-tag": "^2.0.0",
    "graphql": "^14.0.0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.12.2",
    "@graphql-codegen/visitor-plugin-common": "1.12.2",
    "auto-bind": "4.0.0",
    "tslib": "1.11.1"
  },
  "devDependencies": {
    "@ardatan/bob": "0.2.7",
    "@graphql-codegen/testing": "1.12.2",
    "graphql": "14.6.0",
    "jest": "25.1.0",
    "jest-junit": "10.0.0",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "jest-junit": {
    "outputDirectory": "../../../../test-results/typescript-react-apollo"
  },
  "publishConfig": {
    "access": "public"
  }
}
